{
  "spec": {
    "serviceAccountName": "default-editor",
    "containers": [
      {
        "env": [
          {
            "name": "AWS_REGION",
            "value": ""
          },
          {
            "name": "AWS_S3_ENDPOINT",
            "value": "minio-api.kubeflow-minio.svc.cluster.local:9000"
          },
          {
            "name": "AWS_ACCESS_KEY_ID",
            "valueFrom": {
              "secretKeyRef": {
                "name": "minio-root-user",
                "key": "username"
              }
            }
          },
          {
            "name": "AWS_SECRET_ACCESS_KEY",
            "valueFrom": {
              "secretKeyRef": {
                "name": "minio-root-user",
                "key": "password"
              }
            }
          }
        ]
      }
    ]
  }
}